# Immich Configuration
IMMICH_BASE_URL=http://localhost:2283
IMMICH_API_KEY=your-api-key-here

# Script Configuration
DUPLICATES_FILE=duplicates.json
DRY_RUN=true

# API and Caching Configuration
USE_API=true              # Use Immich API (true) or local file only (false)
FORCE_REFRESH=false       # Force refresh from API ignoring cache

# Cron Job Settings
SKIP_CONFIRMATION=false   # Skip user confirmation prompt (for cron jobs)
LOG_LEVEL=INFO           # Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_FILE=                # Optional log file path for cron jobs